{
  "name": "html2vue-renderer",
  "description": "Component is an alternative for v-html directive allows to include components to html markup. It can be useful for rendering fetched html content and to add it some interactivity.",
  "keywords": ["vue.js", "render", "HTML", "vue-component", "component"],
  "private": false,
  "version": "0.0.50",
  "type": "module",
  "types": "./dist/main.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/andyBobro/html2vue-renderer.git"
  },
  "homepage": "https://github.com/andyBobro/html2vue-renderer#readme",
  "main": "./dist/html2vue-renderer.umd.cjs",
  "module": "./dist/html2vue-renderer.js",
  "scripts": {
    "dev": "vite",
    "pub": "yarn build && npm publish",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview"
  },
  "exports": {
    ".": {
      "import": "./dist/html2vue-renderer.js",
      "require": "./dist/html2vue-renderer.umd.cjs"
    }
  },
  "dependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  }
}
